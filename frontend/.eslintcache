stGroupItem>\r\n              <h3>{product.name}</h3>\r\n            </ListGroupItem>\r\n            <ListGroupItem>\r\n              <Rating\r\n                value={product.rating}\r\n                text={`${product.numReviews} Reviews`}\r\n              />\r\n            </ListGroupItem>\r\n            <ListGroupItem>Price : RS.{product.price}</ListGroupItem>\r\n            <ListGroupItem>{product.description}</ListGroupItem>\r\n          </ListGroup>\r\n        </Col>\r\n        <Col md={3} >\r\n          <div className=\"qw1\" >\r\n          <ListGroupItem >\r\n            <Row >\r\n              <Col>Status :</Col>\r\n              <Col>\r\n                {product.countInStock > 0 ? \"In Stock \" : \"out of stock\"}\r\n              </Col>\r\n            </Row>\r\n          </ListGroupItem>\r\n          {product.countInStock > 0 && (\r\n            <ListGroupItem>\r\n              <Row>\r\n                <Col>Qty</Col>\r\n                <Form.Control\r\n                  as=\"select\"\r\n                  value={qty}\r\n                  onChange={(e) => setQty(e.target.value)}\r\n                >\r\n                  {[...Array(product.countInStock).keys()].map((x) => (\r\n                    <option key={x + 1} value={x + 1}>\r\n                      {x + 1}\r\n                    </option>\r\n                  ))}\r\n                </Form.Control>\r\n              </Row>\r\n            </ListGroupItem>\r\n          )}\r\n          <ListGroupItem >\r\n            <div style={{paddingBottom:\"12px\"}}>\r\n            <Button\r\n            \r\n              className=\"btn-block\"\r\n              type=\"button\"\r\n              onClick={addToCartHandler}\r\n            >\r\n              Add to cart\r\n            </Button>\r\n            </div>\r\n          </ListGroupItem>\r\n          </div>\r\n        </Col>\r\n      </Row>\r\n    </>\r\n  );\r\n};\r\n\r\nexport default ProductDetails;\r\n","E:\\youtube\\jaishreeram\\45-shopping-app\\frontend\\sr